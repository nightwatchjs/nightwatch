{
  "name": "nightwatch",
  "description": "Easy to use Node.js based End-to-End testing solution for browser based apps and websites, using the W3C WebDriver API.",
  "version": "2.0.0-alpha.2",
  "author": {
    "name": "Andrei Rusu",
    "email": "andrei@nightwatchjs.org"
  },
  "homepage": "http://nightwatchjs.org",
  "main": "./lib/index.js",
  "license": "MIT",
  "bugs": "https://github.com/nightwatchjs/nightwatch/issues",
  "repository": {
    "type": "git",
    "url": "git@github.com:nightwatchjs/nightwatch.git"
  },
  "dependencies": {
    "assertion-error": "^1.1.0",
    "chai-nightwatch": "^0.4.1",
    "ci-info": "^3.2.0",
    "didyoumean": "^1.2.2",
    "dotenv": "10.0.0",
    "ejs": "^3.1.6",
    "envinfo": "^7.8.1",
    "lodash.clone": "3.0.3",
    "lodash.defaultsdeep": "^4.6.1",
    "lodash.merge": "^4.6.2",
    "minimatch": "3.0.4",
    "minimist": "^1.2.5",
    "mkpath": "1.0.0",
    "mocha": "^9.1.2",
    "ora": "^5.4.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "selenium-webdriver": "^4.0.0-rc-1",
    "semver": "^7.3.5",
    "strip-ansi": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^16.7.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.21",
    "mockery": "~2.1.0",
    "nock": "^10.0.6",
    "nyc": "^15.1.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5",
    "mocha-junit-reporter": "^2.0.0",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0"
  },
  "peerDependencies": {
    "chromedriver": "*",
    "geckodriver": "*"
  },
  "peerDependenciesMeta": {
    "chromedriver": {
      "optional": true
    },
    "geckodriver": {
      "optional": true
    }
  },
  "optionalDependencies": {},
  "bin": {
    "nightwatch": "./bin/nightwatch"
  },
  "man": "",
  "scripts": {
    "eslint": "eslint index.js lib bin examples --quiet && eslint --config ./test/.eslintrc test/ --quiet",
    "mocha-coverage": "nyc --reporter=html mocha test/src/ --recursive",
    "test": "mocha test/src/ --recursive",
    "test-ci": "mocha test/src/ --bail",
    "coverage": "npx nyc report --reporter=text-lcov > ./coverage/mocha_coverage.lcov"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "browser": false,
      "node": true
    },
    "rules": {
      "eqeqeq": "off",
      "curly": [
        "error",
        "all"
      ],
      "quotes": [
        "error",
        "single"
      ]
    }
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "keywords": [
    "nightwatch",
    "nightwatchjs",
    "selenium",
    "testing",
    "webdriver",
    "browserstack",
    "end-to-end",
    "automated-testing",
    "e2e"
  ],
  "files": [
    "bin",
    "examples",
    "lib",
    "api",
    "README.md",
    "LICENSE.md",
    "index.js"
  ]
}
